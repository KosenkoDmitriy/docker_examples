  <span class=3D"c"># Allow only for register SSL (Certbot)
</span>  <span class=3D"n">location</span> ^~ /.<span class=3D"n">well</spa=
n>-<span class=3D"n">known</span>/<span class=3D"n">acme</span>-<span class=
=3D"n">challenge</span> { <span class=3D"n">root</span> /<span class=3D"n">=
var</span>/<span class=3D"n">www</span>/<span class=3D"n">certbot</span>; }

  <span class=3D"c"># Redirect to HTTPS
</span>  <span class=3D"n">location</span> / { <span class=3D"n">return</sp=
an> <span class=3D"m">301</span> <span class=3D"n">https</span>://<span cla=
ss=3D"n">site</span>.<span class=3D"n">com</span>$<span class=3D"n">request=
_uri</span>; }
}
</code></pre></div>



